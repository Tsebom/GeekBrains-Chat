<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.PasswordField?>
<VBox fx:controller="client.RegController"
      xmlns:fx="http://javafx.com/fxml" alignment="center" >

        <TextField fx:id="loginField"  promptText="login" />
        <PasswordField fx:id="passwordField"  promptText="password"/>
        <TextField fx:id="nickField"  promptText="nickname" />
        <TextArea fx:id="textArea" VBox.vgrow="ALWAYS"/>
        <Button text="Registration"  minWidth="100" onAction="#tryToReg"/>
</VBox>
